{
  "name": "re-burlive",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "migrate:content": "ts-node ./scripts/migrate-content.ts",
    "seed:content": "ts-node ./scripts/seed-content.ts",
    "seed:content:dry": "ts-node ./scripts/seed-content.ts --dry",
    "seed:content:apply": "ts-node ./scripts/seed-content.ts --apply",
    "seed:content:update": "ts-node ./scripts/seed-content.ts --apply --update",
    "seed:content:force": "ts-node ./scripts/seed-content.ts --apply --force",
    "seed:content:prune": "ts-node ./scripts/seed-content.ts --apply --prune-tasks",
    "migrate:userid": "ts-node ./scripts/migrate-userid-to-string.ts",
    "migrate:userid:dry": "cross-env DRY_RUN=true ts-node ./scripts/migrate-userid-to-string.ts",
    "migrate:module-ref": "ts-node ./scripts/denormalize-module-ref.ts",
    "migrate:module-ref:dry": "cross-env DRY_RUN=true ts-node ./scripts/denormalize-module-ref.ts",
    "test:regression": "ts-node ./scripts/test-regression.ts",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "validate:content": "ts-node ./scripts/validate-content.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@types/node": "^22.5.5",
    "cross-env": "^10.0.0",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2",
    "zod": "^4.1.5"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^11.1.6",
    "axios": "^1.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^16.3.1",
    "mongoose": "^8.18.0",
    "swagger-ui-express": "^5.0.1",
    "rxjs": "^7.8.2"
  }
}
